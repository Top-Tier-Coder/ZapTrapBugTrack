<!-- Modal -->
<div class="modal fade" id="criticalModal" role="dialog">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Critical Ticket Quick-View</h4>
            </div>
            <div class="modal-body bg-dark">

                @{int countCritical = 0; }
                @foreach (var ticket in ViewBag.ListCritical)
                {
                    var devName = "";
                    if (ticket.DeveloperUser == null)
                    {
                        devName = "No Developer";
                    }
                    else
                    {
                        devName = ticket.DeveloperUser.FullName;
                    }
                    @if (countCritical % 2 == 0)
                    {
                        <div class="row border-bottom bg-gray">
                            <div class="col-11">
                                <div class="row">
                                    <div class="col-6">
                                        <label>Title:</label> @ticket.Title <br />
                                    </div>
                                    <div class="col-6">
                                        <label>Project Name:</label> @ticket.Project.Name
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-6">
                                        <label>Type:</label> @ticket.TicketType.Name <br />
                                    </div>
                                    <div class="col-6">
                                        <label>Status:</label> @ticket.TicketStatus.Name <br />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-6">
                                        <label>Created By:</label> @ticket.OwnerUser.FullName <br />
                                    </div>
                                    <div class="col-6">
                                        <label>Developer</label> @devName <i class="fa fa-arrow-circle-right"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="col-1 mt-2">
                                <a asp-controller="Tickets" asp-action="Details" asp-route-id="@ticket.Id" class="btn btn-info">Go <i class="fa fa-arrow-circle-right"></i></a>
                            </div>
                        </div>
                    }
                    else
                    {
                        <div class="row border-bottom bg-light">
                            <div class="col-11">
                                <div class="row">
                                    <div class="col-6">
                                        <label>Title:</label> @ticket.Title <br />
                                    </div>
                                    <div class="col-6">
                                        <label>Project Name:</label> @ticket.Project.Name
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-6">
                                        <label>Type:</label> @ticket.TicketType.Name <br />
                                    </div>
                                    <div class="col-6">
                                        <label>Status:</label> @ticket.TicketStatus.Name <br />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-6">
                                        <label>Created By:</label> @ticket.OwnerUser.FullName <br />
                                    </div>
                                    <div class="col-6">
                                        <label>Developer</label> @devName <i class="fa fa-arrow-circle-right"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="col-1 mt-2">
                                <a asp-controller="Tickets" asp-action="Details" asp-route-id="@ticket.Id" class="btn btn-info">Go <i class="fa fa-arrow-circle-right"></i></a>
                            </div>
                        </div>

                    }
                    countCritical++; //count = count + 1
                }
            </div>
            <!--end modal body-->
            <div class="modal-footer">

            </div>
        </div>
    </div>
</div>



