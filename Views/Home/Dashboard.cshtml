<section class="content">

    <div class="row">
        <div class="col">


            <!-- DONUT CHARTS -->
            <div class="card card-purple">
                <div class="card-header">
                    <h3 class="card-title">Zap-Trap Stats</h3>

                    <div class="card-tools">
                        <button type="button" class="btn btn-tool" data-card-widget="collapse">
                            <i class="fas fa-minus"></i>
                        </button>
                       
                       
                    </div>
                </div>
                <div class="card-body">
                    <div class="row text-center">
                        <div class="col">
                            <h6>Ticket Status Chart</h6>
                            <canvas id="TicketStatusChart" style="min-height: 250px; height: 250px; max-height: 250px; max-width: 100%;"></canvas>
                        </div>
                        <div class="col">
                            <h6>Ticket Priority Chart</h6>
                            <canvas id="TicketPriorityChart" style="min-height: 250px; height: 250px; max-height: 250px; max-width: 100%;"></canvas>
                        </div>
                        <div class="col">
                            <h6>Ticket Type Chart</h6>
                            <canvas id="TicketTypeChart" style="min-height: 250px; height: 250px; max-height: 250px; max-width: 100%;"></canvas>
                        </div>
                    </div>
                </div>

                <!-- /.card-body -->
            </div>
        </div>
    </div>
    <!-- /.row -->
    <div class="card card-cyan collapsed-card">
        <div class="card-header">
            <h3 class="card-title">Projects Quick-View</h3>
            <div class="card-tools">
                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                    <i class="fas fa-minus"></i>
                </button>
               
            </div>
        </div>
        <div class="card-body">
            <table id="example1" class="table table-bordered table-hover">
                <thead>
                    <tr>
                        <th> Name</th>
                        <th> Company</th>
                        <th> Description</th>
                        <th> ImageFileName</th>
                        <th> ImageFileData</th>
                        <th> Actions</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.Projects)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Company.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Description)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ImageFileName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ImageFileData)
                            </td>
                            <td>
                                <a class="btn btn-warning mb-2" asp-action="Edit" asp-route-id="@item.Id">Editing</a>
                                <a class="btn btn-info mb-2" asp-action="Details" asp-route-id="@item.Id">Details</a>
                                <a class="btn btn-danger mb-2" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
    <div class="card card-purple collapsed-card">
        <div class="card-header">
            <h3 class="card-title">Tickets Quick-View</h3>
            <div class="card-tools">
                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                    <i class="fas fa-minus"></i>
                </button>
            </div>
        </div>
        <div class="card-body">
            <table class="table" id="example1">
