@model ZapTrapBugTrack.Models.DashboardViewModel
<!-- Small boxes (Stat box) -->
<div class="row">
    <div class="col-lg-2 col-6">
        <!-- small box -->
        <div class="small-box bg-primary">
            <div class="inner">
                <h5>Tickets: @ViewBag.Hold</h5>

                <p>Priority: Hold</p>
            </div>
            <div class="icon">
                <i class="fas fa-pause-circle"></i>
            </div>
            <a data-toggle="modal" data-target="#holdModal" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-2 col-6">
        <!-- small box -->
        <div class="small-box bg-info">
            <div class="inner">
                <h5>Tickets: @ViewBag.Low</h5>

                <p>Priority: Low</p>
            </div>
            <div class="icon">
                <i class="fas fa-bug"></i>
            </div>
            <a data-toggle="modal" data-target="#lowModal" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-2 col-6">
        <!-- small box -->
        <div class="small-box bg-success">
            <div class="inner">
                <h5>Tickets: @ViewBag.Medium</h5>

                <p>Priority: Medium</p>
            </div>
            <div class="icon">
                <i class="fas fa-bug"></i>
            </div>
            <a data-toggle="modal" data-target="#mediumModal" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-2 col-6">
        <!-- small box -->
        <div class="small-box bg-warning">
            <div class="inner">
                <h5>Tickets: @ViewBag.High</h5>

                <p>Priority: High</p>
            </div>
